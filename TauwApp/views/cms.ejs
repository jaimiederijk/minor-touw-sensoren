      <% include partials/head %>
      <% include partials/aside %>
      <main class="cms">
          <% if (logged_in !== true) { %>
              <form class="userInput" action="/cms" method="post">
                  <label for="username">Accountnaam</label>
                  <input id="username" type="text" name="username">
                  <label for="password">Wachtwoord</label>
                  <input id="password" type="text" name="password">
                  <input type="submit" value="Submit">
              </form>
          <% } else{ %>
              <h1>Data in Database</h1>

              <form >
                  <label for="search">search</label>
                  <input id="search" type="text" name="search">
                  <input type="submit" value="Submit">
              </form>

            <a href="./cms/add" title="add a new sensor" class="button">Nieuw (+)</a>

              <table>
                  <tr>
                     <th>id</th>
                    <th>Name</th>
                    <th>Sector</th>
                    <th>Branch</th>
                    <th>Link</th>
                    <th>Contact</th>
                    <th>Aanpassen</th>
                    <th>Verwijderen</th>
                  </tr>

                  <% allSensors.forEach(function(sensor) { %>
                      <tr>
                          <td><%= sensor._id %></td>
                          <td><%= sensor.name %></td>
                          <td><%= sensor.sector %></td>
                          <td><%= sensor.branch %></td>
                              <% if(sensor.name !== "" && sensor.sector !== "" && sensor.branch !== ""){ %>
                                   <td><a href="/details/<%= sensor.sector[0]%>/<%= sensor.branch[0] %>/<%= sensor.name.replace(/ /g, '%20') %>">Link</a></td>
                              <% } else{ %>
                                  <td>Link</td>
                              <% } %>
                          <td><%= sensor.contactName %></td>
                          <td><a href="/cms/edit/<%= sensor._id %>">Aanpassen</a></td>
                          <td><a href="/cms/remove/<%= sensor._id %>">Verwijderen</a></td>
                      </tr>
                  <% }) %>
            </table>
          <% } %>
    </main>

  </body>
</html>

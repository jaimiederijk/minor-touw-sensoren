<main>
    <% if(allSensors.length > 0){ %>
        <section>
          <header>
            <h1 class="current">Zoekresultaten voor <%= searchQuery %> <span id="count"></span></h1>
          </header>

          <ul class="branch clearfix">
          <% allSensors.forEach(function(sensorID) { %>
              <% if(sensorID.name !== "" && sensorID.sector !== "" && sensorID.branch !== ""){ %>
                <li class="clearfix">
                    <div class="image-wrap">
                        <% if(sensorID.image.length > 1){ %>
                          <img src="<%= sensorID.image.replace(/ /g, '%20') %>" alt="Image of the <%= sensorID.name %>">
                        <% } else{ %>
                          <img src="/images/xrf.jpg" alt="<%= sensorID.name %>">
                       <% } %>
                    </div>
                  <section>
                        <h4><a href="/details/<%= sensorID.sector %>/<%= sensorID.branch[0] %>/<%= sensorID.name.replace(/ /g, '%20') %>" title="<%= sensorID.name.replace(' ', '%20') %>"><%= sensorID.name %></a></h4>
                        <h4 class="sector"><%= sensorID.sector%></h4>
                    <p><%= sensorID.summary_small %></p>
                    <a href="/details/<%= sensorID.sector %>/<%= sensorID.branch[0] %>/<%= sensorID.name.replace(/ /g, '%20') %>" title="meer..." class="more-url">meer...</a>
                  </section>

                </li>
            <% } %>
          <% }) %>
          </ul>

        </section>
     <% } else{ %>
       <section><p>Geen overeenkomende zoekresultaten gevonden.</p></section>
    <% } %>

</main>
